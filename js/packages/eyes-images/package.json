{
  "name": "@applitools/eyes-images",
  "version": "4.23.0",
  "description": "The simplest implementation of Applitools Eyes SDK, uploads images for validation",
  "keywords": [
    "eyes-images",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-images"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "run --top-level tspc --project ./tsconfig.build.json",
    "generate:tests": "coverage-tests generate ./test/coverage/config.js --name 'eyes-images'",
    "test": "yarn test:coverage",
    "test:coverage": "yarn generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-images' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage run --top-level mocha './test/generic/*.spec.js' --parallel --jobs ${MOCHA_JOBS:-15}",
    "report": "bongo report --name 'js_images' --group 'images'",
    "notify": "dlx --package @applitools/bongo bongo send-release-notification --name 'js_images'"
  },
  "dependencies": {
    "@applitools/eyes": "1.2.14"
  },
  "devDependencies": {
    "@applitools/api-extractor": "workspace:^",
    "@applitools/bongo": "workspace:^",
    "@applitools/sdk-coverage-tests": "workspace:^",
    "@applitools/test-utils": "workspace:^",
    "@types/node": "^12.20.55"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
