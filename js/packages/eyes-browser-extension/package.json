{
  "name": "@applitools/eyes-browser-extension",
  "version": "0.3.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-browser-extension"
  },
  "license": "ISC",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "scripts": {
    "lint": "run --top-level eslint '**/*.js'",
    "build": "yarn build:dist && yarn build:zip",
    "build:dist": "webpack",
    "build:zip": "mkdir ./bin && cd ./dist && zip -r ../bin/eyes-browser-extension.zip ./*",
    "generate:tests": "generic ./test/generic/config.mjs",
    "test": "yarn test:coverage",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-browser-extension' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage run --top-level mocha './test/generated-coverage/*.spec.js' --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "setup": "run --top-level xvfb:setup"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "13.11.14",
    "@applitools/spec-driver-browser-extension": "workspace:*",
    "@applitools/utils": "workspace:*"
  },
  "devDependencies": {
    "@applitools/bongo": "workspace:^",
    "@applitools/generic": "workspace:^",
    "@applitools/spec-driver-playwright": "workspace:^",
    "@applitools/test-utils": "workspace:^",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.18.1",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "net-browserify": "^0.2.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "playwright": "^1.34.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tls-browserify": "^0.2.2",
    "tty-browserify": "^0.0.1",
    "util": "^0.12.3",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
